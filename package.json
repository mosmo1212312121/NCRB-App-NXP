    "build:local": "ng build --base-href=. --prod --configuration=local",
    "build:prod": "ng build --configuration=ncrb --base-href=. --prod",
    "build:test": "npm run e2e && npm run build",
    "build": "ng build --prod --configuration=ncrb --base-href=. && cp -r C://workspace/ATBKIT/NCRB/NCRB-UI/dist/* //Thbnk01ncrbtw1v/NCRB/NCRB-UI",
    "dev": "ng serve --host 0.0.0.0 --port 80",
    "e2e:prod": "del /S /Q .\\reports && npm run e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "e2e:start": "webdriver-manager start --standalone",
    "e2e:update": "webdriver-manager update --proxy http://apac.nics.nxp.com:8080",
    "e2e": "ng e2e --webdriverUpdate=false",
    "format:check": "prettier --ignore-path .gitignore --config ./.prettierrc --write --list-different \"**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint",
    "ng": "ng",
    "precommit": "run-s format:fix lint",
    "predev": "node update-date.js && npm run format:check",
    "postinstall": "npm install --save-dev typescript@3.7.2",
    "serve": "node server.js",
    "set:proxy": "npm config set proxy http://apac.nics.nxp.com:8080/",
    "test:prod": "npm run test -- --no-watch --no-progress --browsers=PhantomCI --code-coverage",
    "test": "ng test"
  },
